{
  "name": "HackBar",
  "version": "0.2.7",
  "description": "HackBar for Chrome",
  "devtools_page": "devtools.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "128": "images/icon.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "page": "background.html"
  },
  "commands": {
    "load_url": {
      "suggested_key": {
        "default": "Alt+A",
        "mac": "MacCtrl+A"
      },
      "description": "Trigger 'Load URL'"
    },
    "split_url": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "MacCtrl+S"
      },
      "description": "Trigger 'Split URL'"
    },
    "execute_url": {
      "suggested_key": {
        "default": "Alt+X",
        "mac": "MacCtrl+X"
      },
      "description": "Trigger 'Execute URL'"
    }
  },
  "manifest_version": 2
}
