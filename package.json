{
  "name": "hackbar",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue ./src/ && prettier --check .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ./src/ && prettier --write .",
    "types:check": "vue-tsc --noEmit",
    "dist": "rm -f $npm_package_name.zip && env -C dist zip -r ../$npm_package_name.zip ."
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.7",
    "crypto-js": "^4.1.1",
    "vue": "^3.2.37",
    "vuetify": "^3.0.0-beta.5"
  },
  "devDependencies": {
    "@mdi/js": "^6.9.96",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "chrome-types": "^0.1.119",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-require-sort": "^1.2.2",
    "eslint-plugin-vue": "^9.1.1",
    "glob": "^8.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-cli-plugin-vuetify": "^2.5.1",
    "vue-tsc": "^0.38.2",
    "yarn-upgrade-all": "^0.7.1"
  },
  "packageManager": "yarn@3.2.1"
}
