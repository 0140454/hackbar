{
  "name": "hackbar",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue ./src/ && prettier --check .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ./src/ && prettier --write .",
    "types:check": "vue-tsc --noEmit",
    "dist": "rm -f $npm_package_name.zip && (cd dist ; zip -r ../$npm_package_name.zip *)"
  },
  "dependencies": {
    "@mdi/font": "^6.7.96",
    "crypto-js": "^4.1.1",
    "roboto-fontface": "*",
    "sass": "^1.52.3",
    "vue": "^3.2.37",
    "vuetify": "^3.0.0-beta.4"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.190",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-require-sort": "^1.2.2",
    "eslint-plugin-vue": "^9.1.1",
    "glob": "^8.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-cli-plugin-vuetify": "~2.5.1",
    "vue-tsc": "^0.38.1"
  }
}
