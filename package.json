{
  "name": "HackBar",
  "description": "A browser extension for Penetration Testing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:chrome": "vite build",
    "build:firefox": "TARGET=firefox vite build",
    "dev:ui": "vite",
    "lint": "eslint --ext .js,.ts,.vue ./src/ && prettier --check .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ./src/ && prettier --write .",
    "types:check": "vue-tsc --noEmit",
    "dist:chrome": "run build:chrome && env -C dist zip ../${npm_package_name}-chrome.zip -FS -r .",
    "dist:firefox": "run build:firefox && env -C dist zip ../${npm_package_name}-firefox.zip -FS -r ."
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.7",
    "@mdi/js": "^6.9.96",
    "crypto-js": "^4.1.1",
    "vue": "^3.2.37",
    "vuetify": "^3.0.0-beta.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-virtual": "^2.1.0",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.3",
    "@types/webextension-polyfill": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "chrome-types": "^0.1.122",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-require-sort": "^1.2.2",
    "eslint-plugin-vue": "^9.2.0",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-cli-plugin-vuetify": "^2.5.1",
    "vue-tsc": "^0.38.3",
    "webextension-polyfill": "^0.9.0",
    "yarn-upgrade-all": "^0.7.1"
  },
  "resolutions": {
    "@types/webextension-polyfill": "patch:@types/webextension-polyfill@npm:^0.9.0#.yarn/patches/@types-webextension-polyfill.patch",
    "vuetify": "patch:vuetify@npm:^3.0.0-beta.5#.yarn/patches/vuetify.patch"
  },
  "packageManager": "yarn@3.2.1"
}
