{
  "name": "hackbar",
  "scripts": {
    "build": "webpack --node-env none --mode none --config ./webpack.config.js",
    "watch": "webpack --node-env development --mode development --watch --config webpack.config.js",
    "lint": "eslint --ext .js,.ts,.vue ./components/ ./scripts/ && prettier --check .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ./components/ ./scripts/ && prettier --write .",
    "pack": "rm -f $npm_package_name.zip && (cd dist ; zip -r ../$npm_package_name.zip *)"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.190",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-require-sort": "^1.2.2",
    "eslint-plugin-vue": "^8.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "sass": "~1.32",
    "sass-loader": "^13.0.0",
    "ts-loader": "^9.3.0",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@mdi/font": "^6.7.96",
    "crypto-js": "^4.1.1",
    "vue": "^2.6.14",
    "vuetify": "^2.6.6"
  }
}
